{
  "coauthorship": false,
  "permissions": {
    "allow": [
      "WebFetch(domain:raw.githubusercontent.com/Metaculus/forecasting-tools*)",
      "WebFetch(domain:github.com/Metaculus/forecasting-tools*)",
      "WebFetch(domain:docs.claude.com)",
      "WebFetch(domain:www.metaculus.com)",
      "WebFetch(domain:docs.polymarket.com)",
      "WebFetch(domain:docs.manifold.markets)",
      "WebFetch(domain:ai.pydantic.dev)",
      "WebFetch(domain:www.metaculus.com)",
      "WebSearch",
      "WebSearch",
      "Bash(grep:*)",
      "Bash(ls *)",
      "Bash(uv run pyright *)",
      "Bash(uv run pytest:*)",
      "Bash(uv run python .claude/plugins/aib/scripts/*)",
      "Bash(uv run ruff *)",
      "Bash(uv run * --help)",
      "Bash(git commit:*)",
      "Bash(git add *)",
      "Bash(git commit *)"
    ],
    "deny": [
      "Read(./**/*.local*)",
      "Bash(uv run python -c *)",
      "Bash(uv run python -c\"*)",
      "Bash(uv run python -c'*)",
      "Bash(uv run python << *)",
      "Bash(uv run python <<'*)",
      "Bash(python *)",
      "Bash(python3 *)"
    ],
    "ask": [
      "Edit(./**/pyproject.toml)"
    ]
  },
  "enabledPlugins": {
    "aib-workflow@local": true,
    "github@claude-plugins-official": true,
    "pyright-lsp@claude-plugins-official": true,
    "agent-sdk-dev@claude-plugins-official": true,
    "commit-commands@claude-plugins-official": true,
    "claude-md-management@claude-plugins-official": true
  },
  "extraKnownMarketplaces": {
    "local": {
      "source": {
        "source": "directory",
        "path": "."
      }
    }
  }
}
